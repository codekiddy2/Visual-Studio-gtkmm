<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <PROJECT_DIR_M>$(PROJECTS_DIR_M)/$(SolutionName)/msvc-$(PlatformToolsetVersion)/$(TargetName)</PROJECT_DIR_M>
    <PROJECT_CMD_M>$(PROJECT_DIR_M)/$(ProjectName).sh</PROJECT_CMD_M>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="PROJECT_DIR_M">
      <Value>$(PROJECT_DIR_M)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="PROJECT_CMD_M">
      <Value>$(PROJECT_CMD_M)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>