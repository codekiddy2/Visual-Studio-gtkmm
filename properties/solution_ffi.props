<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SRC_DIR>$(PROJECTS_DIR)libffi-3.1\</SRC_DIR>
    <SRC_DIR_M>$(PROJECTS_DIR_M)/libffi-3.1</SRC_DIR_M>
    <SOLUTION_NAME>ffi</SOLUTION_NAME>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup>
    <ClCompile />
    <PreBuildEvent />
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <PreBuildEvent />
    <ClCompile />
    <Link />
    <ClCompile />
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="SRC_DIR">
      <Value>$(SRC_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="SRC_DIR_M">
      <Value>$(SRC_DIR_M)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="SOLUTION_NAME">
      <Value>$(SOLUTION_NAME)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>