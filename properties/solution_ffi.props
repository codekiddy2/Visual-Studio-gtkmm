<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SRC_DIR>$(PROJECTS_DIR)libffi-3.1\</SRC_DIR>
    <SRC_DIR_M>$(PROJECTS_DIR_M)/libffi-3.1</SRC_DIR_M>
    <PROJECT_DIR_M>$(SRC_DIR_M)/msvc-$(PlatformToolsetVersion)/$(TargetName)</PROJECT_DIR_M>
    <PROJECT_CMD_M>$(PROJECT_DIR_M)/$(ProjectName).sh</PROJECT_CMD_M>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup>
    <ClCompile />
    <PreBuildEvent />
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <PreBuildEvent />
    <ClCompile />
    <Link />
    <ClCompile>
      <AdditionalIncludeDirectories />
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="SRC_DIR">
      <Value>$(SRC_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="SRC_DIR_M">
      <Value>$(SRC_DIR_M)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="PROJECT_DIR_M">
      <Value>$(PROJECT_DIR_M)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="PROJECT_CMD_M">
      <Value>$(PROJECT_CMD_M)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>